x = 25
def setup():
    size(600,400)
    background(0)


def draw():
    global x
    background(0)
    ellipse(300,200,x,x)
    if keyPressed:
        if key =='a':
            x=x+1
        if key =='l':  
            x=x-1

                   

             



            
            

            
        
         

            
            





        
    
    
